<%- include ("header.ejs") %>
        <div class="box" id="heading">
            <h1><%=  kindOfDay  %></h1> 
        </div>
        <div class="box">
            <% for (let i = 0; i<newItems.length; i++){ %>
                <div class="item">
                <input type="checkbox">
                <p> <%= newItems[i] %> </p>
                </div>
           <% } %> 
             <form class ="item" action="/" method="post">
                <input name="list_item" type="text" placeholder="New Item" autocomplete="off">
                <button  name="list" value= "<%=kindOfDay%>" >+</button>
             </form>
        </div>
<%- include("footer"); %>